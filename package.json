{
  "name": "react-starter",
  "version": "1.0.0",
  "engines": {
    "node": "6.9.3",
    "npm": "4.0.5"
  },
  "description": "React, Redux, React-Router, Webpack 2, PostCss, Mocha + Chai",
  "scripts": {
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "standard": "node ./node_modules/standard/bin/cmd.js ./src/** --fix",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "build": {
      "command": "webpack -p --config webpack.config.production.js",
      "env": {
        "BABEL_ENV": "production"
      }
    },
    "dev": {
      "command": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "watch"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdmatrosov/react-starter.git"
  },
  "keywords": [
    "javascript",
    "react",
    "redux"
  ],
  "author": "Kirill Matrosov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kdmatrosov/react-starter/issues"
  },
  "homepage": "https://kdmatrosov.github.io/react-starter/",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-preset-stage-1": "^6.16.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.0",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "better-npm-run": "0.0.14",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "concurrently": "^3.5.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.29.0",
    "install": "^0.8.9",
    "jsdom": "^9.9.1",
    "mocha": "^3.4.2",
    "npm": "^4.6.1",
    "path": "^0.12.7",
    "postcss": "^5.2.8",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-mixins": "^5.4.1",
    "postcss-simple-vars": "^3.0.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.6.0",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^1.16.5",
    "webpack-shell-plugin": "^0.5.0"
  }
}
